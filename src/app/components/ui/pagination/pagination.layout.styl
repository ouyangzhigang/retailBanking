.paging-panel
    .page-pagination
        padding-left 0
        margin 10px 0
        border-radius 4px
        text-align center
        span
            cursor default
            display block
            height 30px
            line-height 30px
            padding 0 10px
            margin 0 2px
            border-radius 4px
            float left
            outline 0
            text-decoration none
        .current
            cursor default
            height 30px
            line-height 30px
            padding 0 10px
            margin 0 2px
            float left
            text-decoration none
