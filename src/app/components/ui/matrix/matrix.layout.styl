.left-title
    height: 180px;
    float: left;

.matrix
    width: 550px;
    float: left;
    .matrix-item
        width: 180px;
        height: 60px;
        float: left;
        border: 1px solid white;
        vertical-align: middle;
        .matrix-circle
            border: 2px solid #f28286;
            width: 24px;
            height: 24px;
            border-radius: 50%;
            text-align: center;
            vertical-align: middle;
            color: #f28286;
            margin: 8px 43% 2px;
        .matrix-item-title
            height: 22px;
            text-align: center;

.yAxis-title
    margin: 45px 0 0 45px;

.xAxis-title
    margin: 2px 0 0 150px;
    float: left;
